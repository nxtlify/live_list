{
  "name": "live_list",
  "description": "「ニコニコミュニティ 生放送履歴」から直近の生放送一覧を取得するLambda関数",
  "version": "0.1.0",
  "repository": "https://github.com/osm-tc159/live_list.git",
  "author": "Masaya Takeda <844196@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/preset-typescript": "^7.1.0",
    "@types/aws-lambda": "^8.10.17",
    "@types/jsdom": "^12.2.1",
    "fp-ts": "^1.13.0",
    "jsdom": "^13.2.0",
    "netlify-lambda": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.4",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "build": "npm-run-all --print-label --parallel doBuild ci:*",
    "doBuild": "netlify-lambda build --config webpack.config.js src/functions",
    "watch": "COMMUNITY_ID=co1590527 netlify-lambda serve --config webpack.config.js src/functions",
    "ci": "npm-run-all --print-label --parallel ci:*",
    "ci:type": "tsc --project . --noEmit",
    "ci:lint:": "tslint --project . --format stylish"
  }
}
